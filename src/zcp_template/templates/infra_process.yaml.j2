integrations:
  - name: nri-process-discovery
    config:
      # Sample at {{ sample_rate }} second intervals
      interval: {{ sample_rate }}
      
      discovery:
        mode: {{ filter_mode }}
        match_patterns:
          {% for pattern in filter_patterns %}
          - {{ pattern }}
          {% endfor %}
      
      # Advanced settings
      metrics:
        sample_rate: {{ sample_rate }}
        include_thread_metrics: true
        
      {% if exporter_headers %}
      exporter:
        headers:
          {% for key, value in exporter_headers.items() %}
          {{ key }}: {{ value }}
          {% endfor %}
      {% endif %}
